<h1>Login</h1>
<%= form_tag sessions_path do %>
	<div class="field">
      <%= label_tag(:username, "Username:") %><br>
      <%= text_field_tag(:username) %>
    </div>
    <div class="field">
      <%= label_tag(:password, "Password:") %><br>
      <%= password_field_tag(:password) %>
    </div>
    <div class="actions">
      <%= submit_tag("Submit") %>
    </div>
<% end %>