<h1>Listing users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th>Emailverified</th>
      <th>Email</th>
      <th>Zipcode</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user['username'] %></td>
        <td><%= user['password'] %></td>
        <td><%= user['emailVerfied'] %></td>
        <td><%= user['email'] %></td>
        <td><%= user['zipcode'] %></td>
        <td><%= link_to 'Show', user_path(user['objectId']) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
